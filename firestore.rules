rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read and write access for any authenticated user.
    // WARNING: This is insecure for production. You should add more granular
    // rules to protect your data before launching your app.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
